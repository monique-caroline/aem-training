                    jcr:lastModified="{Date}2022-07-30T23:21:20.972-03:00"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd-spa-angular/components/text"
                    text="&lt;p style=&quot;text-align: right;&quot;>&amp;quot;Pantera Negra&amp;quot; atraiu mais pessoas negras, principalmente as que não costumam ir ao cinema, e &amp;quot;Mulher-Maravilha&amp;quot; teve 16% mais espectadores mulheres do que a média dos filmes de super-herói.&lt;/p>&#xd;&#xa;"
                    textIsRich="true"/>
