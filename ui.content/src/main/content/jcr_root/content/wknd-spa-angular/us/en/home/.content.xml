                    text="&lt;h1 style=&quot;text-align: center;&quot;>&lt;b>A-FORCE&lt;/b>&lt;/h1>&#xd;&#xa;"
                    textIsRich="true">
                    <cq:responsive jcr:primaryType="nt:unstructured">
